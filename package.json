{
  "name": "envdot",
  "displayName": "envdot",
  "description": "Get auto-complete suggestions for the variables in .env or .env.local files in workspace",
  "icon": "images/logo.png",
  "version": "0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "publisher": "HDxDev",
  "contributes": {
    "languages": [
      {
        "id": "javascript",
        "aliases": [
          "JavaScript",
          "javascript",
          "js"
        ],
        "extensions": [
          ".js",
          ".jsx"
        ]
      },
      {
        "id": "typescript",
        "aliases": [
          "TypeScript",
          "typescript",
          "ts"
        ],
        "extensions": [
          ".ts",
          ".tsx"
        ]
      }
    ],
    "completions": [
      {
        "language": "javascript",
        "path": "./completions.json"
      },
      {
        "language": "typescript",
        "path": "./completions.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/regex5440/env..git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}
